pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	palt(1, true)
	palt(0, false)
	poke(0x5f2d, 1) -- initiate mouse listener
	p={}
	p.clickdmg=1
	p.autodmg=0
	p.gold=0
	p.lvl=1
	p.hp=1000
	monsters={}
	bglove={}
	bglove.autodmg=1
	bglove.cost=100
	bglove.amount=0
	btree={}
	btree.autodmg=1
	btree.cost=100
	btree.amount=0
	btrainer={}
	btrainer.autodmg=1
	btrainer.cost=100
	btrainer.amount=0
	binn={}
	binn.autodmg=1
	binn.cost=100
	binn.amount=0
	buildings={bglove, btree, btrainer, binn}
 oldb=0
	
	add(monsters, generatemonster(1, 10, 1))
end

function generatemonster(t,h,a)
	m={}
	m.x=64-16
	m.y=20
	m.w=2
	m.h=2
	m.t = t --type
	m.hp= h
	m.atk = a
	m.clock = 0
	return m
end

function onclick(x,y)
	print("hi",0,10,0)
	for m in all(monsters) do
		if(x>=m.x and x<=m.x+m.w*8)then
			if(y>=m.y and y<=m.y+m.h*8)then
				print("click",0,30,0)
    p.gold=p.gold+p.clickdmg
			end
		end
	end
end

function drawmonsters()
	for m in all(monsters) do
		--draw monster m's sprite
  spr(m.t, m.x,m.y,m.w,m.h)
  --draw monster m's life bar
  --rectfill(m.x,m.y+m.h*8+1, m.x+m.w*8,m.y+m.h*8+3,0)
  --rectfill(m.x+1,m.y+m.h*8+2, m.x+m.w*8-1,m.y+m.h*8+2,7)
	end
end

-- x= -1 * (((lefthp*(m.x+m.w*8-1))/fullhp) - m.x - 8*m.w) final formula for damage offset on lifebars?

function autoattack()
		p.gold=p.gold+p.autodmg/30
end


function _update() -- called once per update at 30fps
	cls(7)
 x = stat(32) -- read x coord
	y = stat(33) -- read y coord
	b = stat(34) -- read button bitmask; 1 = primary, 2 = secondary, 4 = middle
	if(b==1 and oldclick==0)onclick(x,y)
 oldclick=b
	if(p.autodmg>0)autoattack()
end

function _draw()	
	drawmonsters()
 print("slimes "..p.gold, 40, 40, 0)
	--circ(x, y, 2, 14) --draw at mouse coord
	

 if(b==1)then
		spr(10, x-2, y)
	else
		spr(9, x-2, y)
	end
	-- print("mouse x "..x.." y "..y, 0,100,0)
	-- print("button "..b,0,58,0)
	-- if(b==1 and oldclick==0)then print("p click",0,50,0)
 -- elseif (b==1 and oldclick==1)then
 --  print("click done",0,50,0)
 -- end
	-- if(b==2)print("s click",0,50,0)
	-- if(b==3)print("p+s click",0,50,0)
	-- if(b==4)print("m click",0,50,0)
	-- if(b==5)print("p+m click",0,50,0)
	-- if(b==6)print("s+m click",0,50,0)
	-- if(b==7)print("p+s+m click",0,50,0)

end

